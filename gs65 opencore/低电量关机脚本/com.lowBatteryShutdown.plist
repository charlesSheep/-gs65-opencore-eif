<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>com.lowBatteryShutdown</string>
	<key>RunAtLoad</key>
	<true/>
	<key>ProgramArguments</key>
	<array>
		<string>/Library/LaunchDaemons/lowBatteryShutdown.sh</string>
	</array>
	<key>StandardOutPath</key>
	<string>/Library/Logs/lowbat_shutdown_log.txt</string>
	<key>StandardErrorPath</key>
	<string>/Library/Logs/lowbat_shutdown_log.txt</string>
</dict>
</plist>
